<AnimationTag>
	<MotoAnimationSet render:id="@+id/animation_sand1"
		render:ordering="together" render:repeat_count="-1">

		<MotoAccelerationAnimation render:type="rotate"
			render:affect_type="set" render:from="90" render:to="20"
			render:acceleration="0.2" render:duration="8000" />

		<MotoLinearAnimation render:type="translate_x"
			render:affect_type="set" render:from="-200" render:to="520"
			render:duration="8000" />
		<MotoAccelerationAnimation render:type="translate_y"
			render:affect_type="set" render:from="400" render:to="-200"
			render:duration="8000" render:acceleration="-38" />
	</MotoAnimationSet>


	<MotoAnimationSet render:id="@+id/animation_sand2"
		render:ordering="together" render:repeat_count="-1">
		<MotoAccelerationAnimation render:type="rotate"
			render:affect_type="set" render:from="20" render:to="-10"
			render:acceleration="0.2" render:duration="8000" />
		<MotoLinearAnimation render:type="translate_x"
			render:affect_type="set" render:from="350" render:to="-250"
			render:duration="8000" />
		<MotoAccelerationAnimation render:type="translate_y"
			render:affect_type="set" render:from="870" render:to="-100"
			render:duration="8000" render:acceleration="30" />
	</MotoAnimationSet>

	<MotoAnimationSet render:id="@+id/animation_sand3"
		render:ordering="together" render:repeat_count="-1">
		<MotoLinearAnimation render:type="rotate"
			render:affect_type="set" render:from="80" render:to="45"
			render:duration="8000" />
		<MotoLinearAnimation render:type="translate_x"
			render:affect_type="set" render:from="-400" render:to="540"
			render:duration="8000" />
		<MotoAccelerationAnimation render:type="translate_y"
			render:affect_type="set" render:from="600" render:to="100"
			render:duration="8000" render:acceleration="-35" />
	</MotoAnimationSet>


</AnimationTag>

