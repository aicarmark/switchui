<?xml version="1.0" encoding="utf-8"?>
<!-- Background resource for digit buttons in the various dialpads
	 used by the Contacts app (see dialpad.xml).
     -->
<selector xmlns:android="http://schemas.android.com/apk/res/android"
          >

    <!-- Even though these two point to the same resource, have two states so the drawable will invalidate itself when coming out of pressed state. -->
    <item android:state_focused="true"  android:state_enabled="false" android:state_pressed="true" android:drawable="@drawable/list_selector_disabled_holo_dark" />
    <item android:state_focused="true"  android:state_enabled="false"                              android:drawable="@drawable/list_selector_disabled_holo_dark" />
    <item android:state_focused="true"                                android:state_pressed="true" android:drawable="@drawable/list_pressed" />
    <item android:state_focused="false"                               android:state_pressed="true" android:drawable="@drawable/list_pressed" />
    <item android:state_focused="true"                                                             android:drawable="@drawable/list_focused" />
    <item                                                                                          android:drawable="@drawable/dial_btn_bknd" />
</selector>

